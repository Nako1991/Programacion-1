Algoritmo enteroOrdenadoAscendente
	Definir num1, num2 Como Entero;
	Escribir "Ingrese el primer numero entero: ";
	Leer num1;
	Escribir "Ingrese el segundo numero entero: ";
	Leer num2;
	
	Si num1 > num2 Entonces
		Escribir num2," ", num1;
	SiNo
		Escribir num1," ", num2;
	FinSi
	
FinAlgoritmo
